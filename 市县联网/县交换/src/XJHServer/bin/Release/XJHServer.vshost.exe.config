<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--采集120数据上报类型:Oracle代表6.4数据库 OracleII代表7.9数据Server代表SqlServer数据库-->
    <add key="DBType" value="Oracle"/>
    <!--120数据库-->
    <add key="DBConnectString" value="Password=lq120new;User ID=lq120new;Data Source=lq120"/>
    <!--GPS监听端口-->
    <add key="GPSServerLocalPort" value="3011"/>
    <!--120业务服务器IP地址-->
    <add key="BusnessServerIP" value="192.168.1.10"/>
    <!--120业务服务器端口-->
    <add key="BusnessServerPort" value="1003"/>
    <!--120业务服务器本地端口-->
    <add key="BusnessServerLocalPort" value="3012"/>
    <!--数据交换通道IP地址-->
    <add key="ExchangeServerIP" value="192.168.1.2"/>
    <!--数据交换通道端口-->
    <add key="ExchangeServerPort" value="9990"/>
    <!--数据上传通道IP地址-->
    <add key="UploadServerIP" value="192.168.1.2"/>
    <!--数据上传通道端口-->
    <add key="UploadServerPort" value="9991"/>
    <!--单位名称-->
    <add key="UnitName" value="xx县120急救指挥中心"/>
    <!--单位编号-->
    <add key="UnitCode" value="001000"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>